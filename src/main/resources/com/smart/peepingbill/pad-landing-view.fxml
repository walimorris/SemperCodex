<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>
<?import javafx.scene.control.Label?>
<BorderPane xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.smart.peepingbill.controllers.PadLandingController">
    <stylesheets>
        <URL value="@peepingStyles.css" />
    </stylesheets>
    <left>
        <VBox fx:id="vboxLeft">
            <Label fx:id="smartNetworkTabLabel" text="Smart System Network"/>
            <Button fx:id="buildNetworkButton" text="Build" onAction="#onBuildNetworkButtonClick"/>
        </VBox>
    </left>
    <center>
        <GridPane fx:id="gridpaneCenter"/>
        <HBox fx:id="gridpaneHbox" alignment="TOP_RIGHT"/>
    </center>
</BorderPane>